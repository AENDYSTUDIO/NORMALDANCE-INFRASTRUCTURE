    "*.css": "tailwindcss",
    "*.scss": "tailwindcss",
    "*.tsx": "typescriptreact",
    "*.ts": "typescript"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.next": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/pnpm-lock.yaml": true,
    "**/package-lock.json": true
  },
